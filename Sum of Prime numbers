#Sum of prime numbers using function definition and function call
def checkPrime(n):
    c=0
    for i in range(1,n+1):
        if(n%i==0):
            c=c+1
    if(c==2):
        return True
    else:
        return False
n=int(input())
sum=0
while(n>0):
    rem=n%10
    if(checkPrime(rem)):
        sum+=rem
    n=n//10
print(sum)
